{
    "name": "sceneindex",
    "fields": [
        {
            "name": "id",
            "type": "Edm.String",
            "key": true
        },
        {
            "name": "start",
            "type": "Edm.Double"
        },
        {
            "name": "end",
            "type": "Edm.Double"
        },
        {
            "name": "shots",
            "type": "Collection(Edm.ComplexType)",
            "fields": [
                {"name": "id", "type": "Edm.Int32"},
                {"name": "start", "type": "Edm.Double"},
                {"name": "end", "type": "Edm.Double"},
                {"name": "tags", "type": "Collection(Edm.String)",
                    "facetable": true},
                {"name": "keyFrames", "type": "Collection(Edm.ComplexType)",
                    "fields": [
                        {"name": "id", "type": "Edm.Int32"},
                        {"name": "thumbnailId", "type" : "Edm.String"},
                        {"name": "start", "type": "Edm.Double"},
                        {"name": "end", "type": "Edm.Double"}
                    ]
                }
            ]
        },
        {
            "name": "video",
            "type": "Edm.ComplexType",
            "fields" :[
                {"name": "id", "type":"Edm.String", "filterable": true},
                {"name": "thumbnailId", "type": "Edm.String"},
                {"name": "detectSourceLanguage", "type": "Edm.Boolean", "filterable" : true, "facetable": true},
                {"name": "sourceLanguages", "type": "Edm.Collection(Edm.String)", "filterable" : true, "facetable": true}
            ]
        },
        { 
            "name": "transcript",
            "type": "Collection(Edm.ComplexType)",
            "fields": [
                {"name": "id", "type": "Edm.Int32"},
                {"name": "text", "type": "Edm.String", "searchable": true},
                {"name": "language", "type": "Edm.String", "filterable": true,
                    "facetable": true},
                {"name": "start", "type": "Edm.Double"},
                {"name": "end", "type": "Edm.Double"}
            ]
        },
        { 
            "name": "faces",
            "type": "Collection(Edm.ComplexType)",
            "fields": [
                {"name": "id", "type": "Edm.Int32"},
                {"name": "name", "type": "Edm.String", "searchable": true, "filterable": true,
                    "facetable": true},
                {"name": "knownPersonId", "type": "Edm.String",
                    "facetable": true},
                {"name": "start", "type": "Edm.Double"},
                {"name": "end", "type": "Edm.Double"},
                {"name": "confidence", "type": "Edm.Double"}
            ]
        },
        { 
            "name": "emotions",
            "type": "Collection(Edm.ComplexType)",
            "fields": [
                {"name": "id", "type": "Edm.Int32"},
                {"name": "type", "type": "Edm.String", "filterable": true,
                    "facetable": true},
                {"name": "start", "type": "Edm.Double"},
                {"name": "end", "type": "Edm.Double"}
            ]
        },
        { 
            "name": "sentiments",
            "type": "Collection(Edm.ComplexType)",
            "fields": [
                {"name": "sentimentType", "type": "Edm.String", "filterable": true,
                    "facetable": true},
                {"name": "averageScore", "type": "Edm.Double"},
                {"name": "start", "type": "Edm.Double"},
                {"name": "end", "type": "Edm.Double"}
            ]
        },
        { 
            "name": "labels",
            "type": "Collection(Edm.ComplexType)",
            "fields": [
                {"name": "id", "type": "Edm.Int32"},
                {"name": "name", "type": "Edm.String", "filterable": true,
                    "facetable": true},
                {"name": "start", "type": "Edm.Double"},
                {"name": "end", "type": "Edm.Double"},
                {"name": "confidence", "type": "Edm.Double"}
            ]
        },
        { 
            "name": "audioEffects",
            "type": "Collection(Edm.ComplexType)",
            "fields": [
                {"name": "type", "type": "Edm.String", "filterable": true,
                    "facetable": true},
                {"name": "start", "type": "Edm.Double"},
                {"name": "end", "type": "Edm.Double"}
            ]
        },
        {
            "name": "playlist",
            "type": "Edm.ComplexType",
            "fields": [
                {"name": "name", "type": "Edm.String", "filterable": true },
                {"name": "description", "type": "Edm.String", "filterable": false },
                {"name": "privacyMode", "type": "Edm.String", "filterable": true, "facetable": true },
                {"name": "state", "type": "Edm.String", "filterable": true, "facetable": true },
                {"name": "accountId", "type": "Edm.String", "filterable": true, "facetable": true },
                {"name": "id", "type": "Edm.String", "filterable": true },
                {"name": "userName", "type": "Edm.String", "filterable": true, "facetable": true },
                {"name": "created", "type": "Edm.DateTimeOffset", "filterable": true},
                {"name": "isOwned", "type": "Edm.Boolean", "filterable": true, "facetable": true },
                {"name": "isEditable", "type": "Edm.Boolean", "filterable": true, "facetable": true },
                {"name": "isBase", "type": "Edm.Boolean", "filterable": true, "facetable": true },
                {"name": "durationInSeconds", "type": "Edm.Double", "filterable": true}
            ]
        }
    ]
}